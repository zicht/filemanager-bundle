{
    "name": "zicht/filemanager-bundle",
    "description": "Zicht FileManagerBundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Gerard van Helden",
            "email": "gerard@zicht.nl"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/symfony": "^3.4",
        "twig/twig": "^1.40 || ^2.9",
        "jms/metadata": "^1.5",
        "zicht/util": "^1.7",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1",
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\FileManagerBundle\\": [
                "src/Zicht/Bundle/FileManagerBundle/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\FileManagerBundle\\": [
                "tests/ZichtTest/Bundle/FileManagerBundle/"
            ]
        }
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
